<div id="middleCol">
<h1>Editing FAQ Categories</h1>

<%= form_for(@faq_category) do |f| %>
<% if @faq_category.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@faq_category.errors.count, "error") %> prohibited this event from being saved:</h2>

    <ul>
    <% @faq_category.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
  <p>
    <%= f.label :category %><br />
    <%= f.text_field :category %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', faq_categories_path, :class=>"admin" %>
</div>