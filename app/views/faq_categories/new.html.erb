<div id="centerCol">
<h1>New FAQ Category</h1>

<% form_for(@faq_category) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :category %><br />
    <%= f.text_field :category %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', faq_categories_path, :class=>"admin" %>
</div>